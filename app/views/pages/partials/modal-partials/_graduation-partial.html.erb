<!-- Modal content-->
<div class="modal-content">
  <div class="modal-header">
    <h4 class="modal-title">Graduation Photoshoots</h4>
  </div>


  <div class="modal-body">
    
    <!--- carsoul -->

      <div class="container" style="width: 100%">
        <div id="myCarouselGraduation" class="carousel slide" data-ride="carousel">
          <!-- Indicators -->
          <ol class="carousel-indicators">
            <li data-target="#myCarouselGraduation" data-slide-to="0" class="active"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="1"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="2"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="3"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="4"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="5"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="6"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="7"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="8"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="9"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="10"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="11"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="12"></li>
            <li data-target="#myCarouselGraduation" data-slide-to="13"></li>
          </ol>

          <!-- Wrapper for slides -->
          <div class="carousel-inner">
            <div class="item active">
              <img style=" width:100%" src="<%= asset_path('grad/g0.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g2.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g3.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g7.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g4.jpg')%>" >
            </div>
            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g5.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g6.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g8.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g9.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g11.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g12.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g13.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g14.jpg')%>" >
            </div>

            <div class="item">
              <img style=" width:100%" src="<%= asset_path('grad/g15.jpg')%>" >
            </div>

          </div>

          <!-- Left and right controls -->
          <a class="left carousel-control" href="#myCarouselGraduation" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control" href="#myCarouselGraduation" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>

  </div>


  <div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
  </div>
  
</div>

