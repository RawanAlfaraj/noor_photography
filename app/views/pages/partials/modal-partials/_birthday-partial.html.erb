<!-- Modal content-->
<div class="modal-content">
  <div class="modal-header">
    <h4 class="modal-title">Birthdays Photoshoots</h4>
  </div>


  <div class="modal-body">
    
    <!--- carsoul -->

      <div class="container" style="width: 100%">
        <div id="myCarouselBirthday" class="carousel slide" data-ride="carousel">
          <!-- Indicators -->
          <ol class="carousel-indicators">
            <li data-target="#myCarouselBirthday" data-slide-to="0" class="active"></li>
            <li data-target="#myCarouselBirthday" data-slide-to="1"></li>
            <li data-target="#myCarouselBirthday" data-slide-to="2"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="3"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="4"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="5"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="6"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="7"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="8"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="9"></li>
            <li data-target="#myCarouselOutdoor" data-slide-to="10"></li>
          </ol>

          <!-- Wrapper for slides -->
          <div class="carousel-inner">
            <div class="item active">
              <img style=" width:100%" src="<%= asset_path('birthday/b00.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b2.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b3.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b4.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b5.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b7.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b6.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b8.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b10.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b17.jpg')%>" >
            </div>

            <div class="item">
               <img style=" width:100%" src="<%= asset_path('birthday/b18.jpg')%>" >
            </div>

          </div>

          <!-- Left and right controls -->
          <a class="left carousel-control" href="#myCarouselBirthday" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control" href="#myCarouselBirthday" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>

  </div>


  <div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
  </div>
  
</div>

